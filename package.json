{
  "name": "semantic-styles-react",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@reach/accordion": "^0.15.1",
    "@reach/alert-dialog": "^0.15.0",
    "@reach/combobox": "^0.15.1",
    "@reach/dialog": "^0.15.0",
